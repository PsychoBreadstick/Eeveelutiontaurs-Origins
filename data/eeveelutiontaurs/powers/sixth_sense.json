{
    "name": "Sixth Sense",
    "description": "Due to your fine and sensitive hairs, youâ€™re capable of sensing all entities around you in a 32 block radius.",
    "type": "origins:multiple",
    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.primary_active",
            "continuous": false
        },
        "retain_state": true
    },
    "scent": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:entity_glow",
        "bientity_condition": {
            "type": "origins:distance",
            "comparison": "<=",
            "compare_to": 32
        },
        "use_teams": false,
        "red": 0.9,
        "green": 0.7,
        "blue": 1
    },
    "scent_info": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:action_over_time",
        "interval": 5,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "title @s actionbar [\"\",{\"text\":\"Sixth Sense: \"},{\"text\":\"ON\",\"color\":\"green\"}]"
                }
            ]
        },
        "falling_action": {
            "type": "origins:execute_command",
            "command": "title @s actionbar [\"\",{\"text\":\"Sixth Sense: \"},{\"text\":\"OFF\",\"color\":\"red\"}]"
        }
    }
}