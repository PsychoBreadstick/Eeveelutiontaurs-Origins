    "espeontaur": {
        "condition": {
            "inverted": true,
            "type": "origins:sneaking"
        },
        "type": "origins:edible_item",
        "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
                "item": "minecraft:phantom_membrane"
            }
        },
        "food_component": {
            "hunger": 0,
            "saturation": 0,
            "always_edible": true
        },
        "consume_animation": "drink",
        "consume_sound": "minecraft:block.amethyst_block.fall",
        "consuming_time_modifier": {
            "operation": "set_total",
            "value": 2
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:entity.player.levelup",
                    "volume": 2,
                    "pitch": 0.6
                },
                {
                    "type": "origins:execute_command",
                    "command": "origin set @s origins:origin eeveelutiontaurs:espeontaur"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:end_rod ~ ~0.5 ~ 1 1 1 0.2 200"
                },
                {
                    "type": "origins:execute_command",
                    "command": "title @s actionbar [{\"text\":\"You Have Evolved Into: \"},{\"text\":\"Espeontaur\",\"color\":\"dark_purple\"}]"
                }
            ]
        },
        "priority": 0
    },
